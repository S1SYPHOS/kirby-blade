{
    "name": "afbora/kirby-blade",
    "description": "Enable Laravel Blade Template Engine for Kirby 3",
    "keywords": [
        "kirby", "kirby-3", "kirby-plugin" ,"kirby-blade", "blade", "laravel", "view", "template", "template-engine"
    ],
    "version": "1.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet Bora",
            "email": "ahmet.bora@pixelturk.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "getkirby/composer-installer": "^1.1",
        "illuminate/view": "^5.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "config.php",
            "helpers.php"
        ],
        "psr-4": {
            "Afbora\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
